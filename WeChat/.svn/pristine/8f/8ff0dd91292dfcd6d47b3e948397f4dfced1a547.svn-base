/*loading*/
#loadingControl{width:50px; height:50px;position: fixed; top:50%; left:50%; margin:-25px 0 0 -25px; z-index:99999;}


.bg-opacity{ background: #000; opacity: 0.5; height: 100%; width: 100%; position: absolute; top: 0; left: 0; z-index: 1}
