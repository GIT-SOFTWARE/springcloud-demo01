/*page*/
.loadMore{ text-align: center; padding: 0.5rem 0;}


/*上拉刷新*/
.loadUp{display:inline-block;width:16px;height:30px;background:url("../images/loadUp.png") no-repeat center 4px;vertical-align: middle;-webkit-background-size:12px 18px;-webkit-transform-origin:center center;}
.loadDown{display:inline-block;width:16px;height:30px;background:url("../images/loadDown.png") no-repeat center 4px;vertical-align: middle;-webkit-background-size:12px 18px;-webkit-transform-origin:center center;}
#loadMoreText{display:inline-block;height:20px;line-height:20px;}
.loadUp,.loadDown {-webkit-animation-duration:200ms;-webkit-animation-fill-mode:both;-webkit-animation-timing-function:ease-in-out;}
.loadDown {-webkit-animation-name:slideUpToDown;}
.loadUp {-webkit-animation-name:slideDownToUp;}
@-webkit-keyframes slideUpToDown { 0% {-webkit-transform: rotate(-180deg);} 100% {-webkit-transform: rotate(-360deg);}}