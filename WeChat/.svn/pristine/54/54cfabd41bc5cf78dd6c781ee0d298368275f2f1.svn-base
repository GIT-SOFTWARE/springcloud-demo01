	<!-- 统一定义版本 maven的变量值 -->
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<spring-version>3.1.1.RELEASE</spring-version>
		<hibernate-version>3.5.2-Final</hibernate-version>
		<skipTests>true</skipTests>
		<glassfish.embedded-static-shell.jar>${glassfish.home}/glassfish/lib/embedded/glassfish-embedded-static-shell.jar</glassfish.embedded-static-shell.jar>
	</properties>


	<!-- 设置依赖包所在的maven nexus仓库 -->
	<repositories>
		<repository>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
			<id>internal-repo</id>
			<name>Internal repository of mama100.com</name>
			<url>http://192.168.235.156:8081/nexus/content/groups/public/</url>
		</repository>
	</repositories>


